class Student {
    int id;
    String name;

    Student(int id, String name) {
        this.id = id;
        this.name = name;
    }
//JAVA ADVANCED CLASS AND FEATURE
    void show() {
        System.out.println(id + " " + name);
    }

    public static void main(String[] args) {
        Student s1 = new Student(1, "ATUL SIR");
        s1.show();
    }
}
